{
    "name": "起点",
    "url": "www.qidian.com",
    "version": 100,
    "search":{
        "url":"https://www.qidian.com/so/{{key}}.html?page={{page}}",
        "charset":"utf-8",
        "list":"class.res-book-item jsAutoReport",
        "name":"class.book-info-title@text",
        "author":"class.author@a.0@text",
        "detail":"class.book-img-box@a@href"
    },
    "detail": {
        "name": "div.book-info-top h1",
        "catalog": "p.catalog-header-infos > a.href"
    },
    "catalog": {
        "list": ".volume-chapters > li",
        "orderBy": 0,
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": ".relative > p"
    }
}
